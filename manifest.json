{
  "manifest_version": 2,

  "name": "Three-Hub",
  "description": "This extension allows you to see 3d models from github directly",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://www.github.com/*", "*://github.com/*"],
      "js": [
        "vendor/jquery-2.0.3.js",
        "vendor/three.js",
        "vendor/stats.min.js",
        "lib/app.js"

      ]
    }
  ],
  "permissions": [
    "https://api.github.com/"
  ]
}
